    }
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.19.0",
    "del": "^6.0.0",
    "electron-builder": "^22.10.5",
    "eslint": "^7.23.0",
    "eslint-plugin-modules-newline": "^0.0.6",
