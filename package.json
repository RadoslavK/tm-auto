{
  "name": "tm-auto",
  "version": "1.0.0",
  "author": "Buckyx",
  "main": "dist/main/index.js",
  "title": "TM Auto",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=13.7"
  },
  "scripts": {
    "generate-types": "ts-node --compiler-options={\\\"module\\\":\\\"CommonJS\\\"} generateGraphqlTypes",
    "lint": "eslint --cache \"src/**/*\"",
    "lint-errors": "yarn lint --quiet",
    "lint-fix-errors": "yarn lint-errors --fix",
    "typecheck": "tsc",
    "build-server": "set NODE_ENV=production&& webpack --config webpack.server.config.js",
    "build-main": "set NODE_ENV=production&& webpack --config webpack.main.config.js",
    "build-renderer": "set NODE_ENV=production&& webpack --config webpack.renderer.config.js",
    "build": "set NODE_ENV=production&& yarn build-main&& yarn build-renderer&& yarn build-server",
    "release": "electron-builder --dir",
    "start-m": "webpack --config webpack.server.config.js&& webpack --config webpack.main.config.js && electron .webpack/main --remote-debugging-port=9222 --inspect",
    "start-r": "node hot-reload.server.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "tm.auto",
    "asar": false,
    "productName": "TM Auto",
    "copyright": "Copyright Â© 2020 ${author}",
    "win": {
      "target": "portable"
    },
    "icon": "./resources/images/TMAuto256.png",
    "directories": {
      "output": "pack"
    },
    "files": [
      "dist/**/*"
    ],
    "nodeVersion": "current"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.2",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.0",
    "debounce": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.0.8",
    "iterall": "^1.3.0",
    "node-ipc": "^9.1.1",
    "pubsub-js": "^1.8.0",
    "puppeteer": "^3.1.0",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-stealth": "^2.4.9",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "typeface-roboto": "^0.0.75",
    "uuid": "^8.1.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/fragment-matcher": "^1.15.0",
    "@graphql-codegen/near-operation-file-preset": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@graphql-codegen/typescript-react-apollo": "^1.15.0",
    "@graphql-codegen/typescript-resolvers": "^1.15.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.2",
    "@types/debounce": "^1.2.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "@types/node-ipc": "^9.1.3",
    "@types/pubsub-js": "^1.8.0",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.0.0",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "eslint-plugin-tm-auto": "file:./_eslint",
    "eslint-plugin-unicorn": "^20.0.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
