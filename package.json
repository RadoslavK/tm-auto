{
  "name": "tm-auto",
  "version": "1.0.0",
  "author": "Buckyx",
  "title": "TM Auto",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "workspaces": ["packages/*"],
  "scripts": {
    "app": "yarn workspace app",
    "main": "yarn workspace main",
    "server": "yarn workspace server",
    "renderer": "yarn workspace renderer",
    "shared": "yarn workspace shared",
    "renderer-typecheck": "yarn renderer typecheck",
    "lint": "eslint --cache \"src/**/*\"",
    "lint-errors": "yarn lint --quiet",
    "lint-fix-errors": "yarn lint-errors --fix",
    "build-server": "set NODE_ENV=production&& webpack --config webpack.server.config.js",
    "build-main": "set NODE_ENV=production&& webpack --config webpack.main.config.js",
    "build-renderer": "set NODE_ENV=production&& webpack --config webpack.renderer.config.js",
    "build": "concurrently \"yarn build-mai\" \"yarn build-renderer\" \"yarn build-server\"",
    "start-r": "set NODE_ENV=development&& webpack serve --config webpack.renderer.config.js",
    "relay": "relay-compiler"
  }
}
