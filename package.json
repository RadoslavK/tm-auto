{
  "name": "tm-auto",
  "version": "1.0.0",
  "author": "Buckyx",
  "main": ".webpack/main",
  "title": "TM Auto",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "lint": "eslint --cache \"src/**/*\"",
    "lint-errors": "yarn lint --quiet",
    "lint-fix-errors": "yarn lint-errors --fix",
    "typecheck": "tsc",
    "build-server": "set NODE_ENV=production&& webpack --config webpack.server.config.js",
    "build-main": "set NODE_ENV=production&& webpack --config webpack.main.config.js",
    "build-renderer": "set NODE_ENV=production&& webpack --config webpack.renderer.config.js",
    "build": "concurrently \"yarn build-mai\" \"yarn build-renderer\" \"yarn build-server\"",
    "pack": "yarn release --dir",
    "release": "yarn build&& electron-builder",
    "start-m": "set NODE_ENV=development&& webpack --config webpack.server.config.js&& webpack --config webpack.main.config.js && electron .webpack/main --remote-debugging-port=9222 --inspect",
    "start-r": "set NODE_ENV=development&& webpack serve --config webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "relay": "relay-compiler",
    "nexus": "set shouldGenerateArtifacts=true&& ts-node-dev --transpile-only --project src/main/tsconfig.json --no-notify --respawn src/main/server/_api/schema.ts",
    "typecheck-main": "tsc --project src/main/tsconfig.json",
    "typecheck-renderer": "tsc --project src/renderer/tsconfig.json"
  },
  "build": {
    "appId": "tm.auto",
    "asar": false,
    "productName": "TM Auto",
    "copyright": "Copyright Â© 2019 - 2021 ${author}",
    "icon": "./resources/images/TMAuto256.png",
    "directories": {
      "output": "pack"
    },
    "win": {
      "target": "nsis"
    },
    "publish": null,
    "files": [
      "dist/**/*"
    ],
    "nodeVersion": "current"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "debounce": "^1.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "^15.5.0",
    "graphql-subscriptions": "^1.2.1",
    "iterall": "^1.3.0",
    "jsdom": "^16.5.1",
    "jszip": "^3.6.0",
    "make-promises-safe": "^5.1.0",
    "nanoid": "^3.1.21",
    "nexus": "^1.0.0",
    "node-ipc": "^9.1.4",
    "pubsub-js": "^1.9.3",
    "puppeteer-core": "^4.0.1",
    "puppeteer-extra": "^3.1.17",
    "puppeteer-extra-plugin-stealth": "^2.7.5",
    "react": "^17.0.1",
    "react-dnd": "^14.0.1",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.1",
    "react-relay": "^11.0.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.22.3",
    "relay-runtime": "^11.0.0",
    "source-map-support": "^0.5.19",
    "typeface-roboto": "^1.1.13",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/debounce": "^1.2.0",
    "@types/jsdom": "^16.2.7",
    "@types/node": "^14.14.34",
    "@types/node-ipc": "^9.1.3",
    "@types/pubsub-js": "^1.8.2",
    "@types/puppeteer-core": "^2.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dnd-html5-backend": "^3.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-relay": "^7.0.17",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized": "^9.21.11",
    "@types/relay-runtime": "^10.1.9",
    "@types/which": "^1.3.2",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-relay": "^11.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.3",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "eslint": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-relay": "^1.8.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "relay-compiler": "^11.0.0",
    "relay-compiler-language-typescript": "^13.0.6",
    "relay-config": "^11.0.0",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
