    "typescript": "^3.7.5",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "zen-observable-ts": "^0.8.21"
  }
}
