{
  "name": "tm-auto",
  "version": "1.0.0",
  "author": "Buckyx",
  "main": ".webpack/main",
  "license": "MIT",
  "engines": {
    "node": ">=13.7"
  },
  "scripts": {
    "debug-webpack-renderer-config": "node-nightly --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./webpack.renderer.config.js",
    "debug": "electron-forge start -- --remote-debugging-port=9222 --inspect",
    "generate:types": "graphql-codegen --config codegen.yml",
    "lint": "eslint \"src/**/*\"",
    "lint-errors": "eslint \"src/**/*\" --quiet",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-utilities": "^1.3.4",
    "clsx": "^1.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^6.0.0",
    "iterall": "^1.3.0",
    "merge-graphql-schemas": "^1.7.8",
    "node-ipc": "^9.1.1",
    "puppeteer": "^3.1.0",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-stealth": "^2.4.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "typeface-roboto": "^0.0.75",
    "uuid": "^8.1.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.51",
    "@graphql-codegen/cli": "^1.14.0",
    "@graphql-codegen/fragment-matcher": "^1.14.0",
    "@graphql-codegen/typescript": "^1.14.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.14.0",
    "@graphql-codegen/typescript-operations": "^1.14.0",
    "@graphql-codegen/typescript-resolvers": "^1.14.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/node": "^14.0.5",
    "@types/node-ipc": "^9.1.3",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.0.0",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-tm-auto": "file:./_eslint",
    "eslint-plugin-typescript-sort-keys": "^0.10.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "node-loader": "^0.6.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.5",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "zen-observable-ts": "^0.8.21"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_webpack"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/client/static/index.html",
                  "js": "./src/client/app/index.tsx",
                  "preload": {
                    "js": "./src/client/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
