{
  "name": "server",
  "version": "1.0.0",
  "author": "Buckyx",
  "private": true,
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "scripts": {
    "nexus": "set shouldGenerateArtifacts=true&& set TS_NODE_PROJECT=src/main/tsconfig.json&& nodemon --config nodemon.nexus.json",
    "build": "tsc --build",
    "start": "set NODE_ENV=development&& tsc --project packages/main/tsconfig.json && electron . --remote-debugging-port=9222 --inspect",
    "typecheck": "tsc --noEmit",
    "lint": ""
  },
  "dependencies": {
    "debounce": "^1.2.1",
    "graphql": "^15.5.0",
    "graphql-subscriptions": "^1.2.1",
    "iterall": "^1.3.0",
    "jsdom": "^16.5.1",
    "jszip": "^3.6.0",
    "make-promises-safe": "^5.1.0",
    "nexus": "^1.0.0",
    "node-ipc": "^9.1.4",
    "pubsub-js": "^1.9.3",
    "puppeteer-core": "^4.0.0",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-stealth": "^2.4.9"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/jsdom": "^16.2.7",
    "@types/node-ipc": "^9.1.3",
    "@types/pubsub-js": "^1.8.2",
    "@types/puppeteer-core": "^2.0.0",
    "typescript": "^4.2.3"
  }
